{
  "name": "v3-skeleton",
  "private": false,
  "version": "2.0.0",
  "type": "module",
  "author": {
    "name": "Aar√≥n Montes"
  },
  "license": "MIT",
  "files": [
    "dist",
    "src"
  ],
  "types": "dist/index.d.ts",
  "repository": {
    "url": "https://github.com/ajomuch92/vue3-skeleton"
  },
  "main": "./dist/v3-skeleton.umd.cjs",
  "module": "./dist/v3-skeleton.js",
  "exports": {
    ".": {
      "import": "./dist/v3-skeleton.js",
      "require": "./dist/v3-skeleton.umd.cjs"
    },
    "./dist/index.css": {
      "import": "./dist/index.css",
      "require": "./dist/index.css"
    }
  },
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build  && npm run build:types",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit",
    "build:types": "vue-tsc --declaration --emitDeclarationOnly --outDir dist"
  },
  "dependencies": {
    "vue": "^3.5.10"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.1.4",
    "typescript": "^5.9.3",
    "vite": "^5.4.8",
    "vite-plugin-lib-inject-css": "^2.1.1",
    "vue-tsc": "^3.1.5"
  },
  "keywords": [
    "Vue",
    "Vue 3",
    "skeleton",
    "loader",
    "shimmer"
  ]
}
